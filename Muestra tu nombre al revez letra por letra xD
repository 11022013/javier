package ejercicio.chartat;


public class EjercicioChartat {

    public static void main(String[] args) {
     
        String nombre="Javier";
        
        for(int i=nombre.length()-1;i>=0;i--) {
            
            System.out.println(nombre.charAt(i));
            
        }
        
    }
    
}

/*
r
e
i
v
a
J
*/
